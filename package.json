{
  "name": "stmgr",
  "private": true,
  "version": "1.0.0",
  "description": "live stream manager",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "prod": "node dist/main.js",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "gen": "drizzle-kit generate --config=drizzle-prod.config.ts",
    "mig": "drizzle-kit migrate --config=drizzle-prod.config.ts",
    "push": "drizzle-kit push --config=drizzle-dev.config.ts",
    "export": "drizzle-kit export --config=drizzle-prod.config.ts"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.831.0",
    "@bull-board/api": "^6.11.1",
    "@bull-board/express": "^6.11.1",
    "@bull-board/nestjs": "^6.11.1",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.1",
    "bullmq": "^5.56.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.39.0",
    "ioredis": "^5.6.1",
    "js-yaml": "^4.1.0",
    "jslog": "git+https://github.com/rwiv/jslog.git#v1.0.2",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@faker-js/faker": "^9.8.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.8",
    "@types/amqplib": "^0.10.6",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.30.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "unplugin-swc": "^1.5.1",
    "vitest": "^3.0.2"
  }
}
